{
	"name":"HorizonModule",
	"type":"background",
	"description":"Switch channels on the UPC Horizon box",
	"enable":"true",
	"android": {
		"package":"com.dobots.examplemodule",
		"url":"https://play.google.com/store/apps/details?id=com.dobots.examplemodule"
	},
	"ports": [
		{
			"name":"channel",
			"dir":"in",
			"type":"int",
			"middleware":"default"
		},
		{
			"name":"command",
			"dir":"in",
			"type":"std::string",
			"middleware":"default"
		},
		{
			"name":"address",
			"dir":"in",
			"type":"std::string",
			"middleware":"default"
		}
	]
}
