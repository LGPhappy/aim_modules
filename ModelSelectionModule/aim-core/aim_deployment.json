{
	"name":"ModelSelectionModule",
	"type":"background",
	"description":"Select a model using e.g. AIC or BIC",
	"enable":"false",
	"android": {
		"package":"com.dobots.examplemodule",
		"url":"https://play.google.com/store/apps/details?id=com.dobots.examplemodule"
	},
	"ports": [
		{
			"name":"residuals",
			"dir":"in",
			"type":"intarray",
			"middleware":"standard"
		},
		{
			"name":"modelparametercount",
			"dir":"in",
			"type":"int",
			"middleware":"standard"
		},
		{
			"name":"method",
			"dir":"in",
			"type":"int",
			"middleware":"standard"
		},
		{
			"name":"modelquality",
			"dir":"out",
