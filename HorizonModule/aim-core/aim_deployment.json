{
	"name":"HorizonModule",
	"type":"background",
	"description":"Switch channels on the UPC Horizon box",
	"version":"0.1",
	"supported_middleware":[ ],
	"supported_devices":[ ],
	"enable":"true",
	"android": {
		"package":"org.dobots.horizonmodule",
		"url":"https://play.google.com/store/apps/details?id=org.dobots.horizonmodule"
	},
	"ports": [
		{
			"name":"channel",
			"dir":"in",
			"type":"int",
			"middleware":"default"
		},
		{
			"name":"command",
			"dir":"in",
			"type":"std::string",
			"middleware":"default"
		},
		{
			"name":"address",
			"dir":"in",
			"type":"std::string",
			"middleware":"default"
		}
	]
}
